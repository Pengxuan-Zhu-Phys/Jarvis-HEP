#!/usr/bin/env python3 

import os, sys 
pwd = os.path.abspath(os.path.dirname(__file__))
sys.path.append(os.path.join(pwd, "src"))
from core import Core 

def main():
    jc = Core()
    jc.initialization()
    if jc.scan_mode:
        jc.run_sampling()
    elif jc.args.cvtDB:
        jc.convert()


if __name__ == "__main__":
    main()