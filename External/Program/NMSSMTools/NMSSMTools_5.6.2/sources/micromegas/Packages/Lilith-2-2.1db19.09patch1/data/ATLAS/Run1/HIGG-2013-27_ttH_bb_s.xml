<!--
ATLAS result for mu(ttH, bb) at mH = 125. GeV
https://atlas.web.cern.ch/Atlas/GROUPS/PHYSICS/PAPERS/HIGG-2013-27/
 -->

<expmu decay="bb" dim="1" type="n">
  <experiment>ATLAS</experiment>
  <source type="published">HIGG-2013-27</source>
  <sqrts>7+8</sqrts>
  <mass>125.</mass>

  <eff prod="ttH">1</eff>

  <bestfit>1.5</bestfit>
  
  <param>
    <uncertainty side="left">-1.1</uncertainty>
    <uncertainty side="right">1.1</uncertainty>
    <!-- if not needed, just <uncertainty></uncertainty> -->
  </param>
</expmu>
